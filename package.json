{
  "name": "@carbon/ai",
  "version": "0.0.1-alpha.0",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "description": "Carbon for AI",
  "license": "Apache-2.0",
  "repository": "https://github.com/carbon-design-system/carbon-for-ai",
  "main": "es/index",
  "types": "es/index",
  "module": "es/index",
  "files": [
    "dist",
    "es",
    "index"
  ],
  "scripts": {
    "build": "vite build --mode production && pnpm tsc",
    "build:dev": "vite build --mode development",
    "build-storybook": "storybook build",
    "format": "prettier --check '**/*.{js,md,scss,ts,tsx}' '!**/*.snap.js' '!**/{build,es,lib,.storybook,ts,umd,storybook-static}/**'",
    "format:write": "prettier --cache --write '**/*.{js,md,scss,ts,tsx}' '!**/*.snap.js' '!**/{build,es,lib,.storybook,ts,umd,storybook-static}/**'",
    "format:staged": "prettier --list-different '**/*.{js,md,scss,ts,tsx}' '!**/*.snap.js' '!**/{build,es,lib,storybook,ts,umd}/**' '!src/components/**'",
    "lint": "eslint src --ext .js,.ts",
    "lint:license": "tasks/check-license.cjs -a",
    "lint:license:staged": "tasks/check-license.cjs -w",
    "lint:styles": "stylelint '**/*.{css,scss}' --report-needless-disables --report-invalid-scope-disables",
    "storybook": "storybook dev -p 6006",
    "prepare": "husky install",
    "test": "pnpm build && web-test-runner \"src/components/**/*.test.ts\" --node-resolve --playwright --browsers chromium firefox webkit",
    "test:updateSnapshot": "pnpm build && web-test-runner \"src/components/**/*.test.ts\" --node-resolve --update-snapshots"
  },
  "dependencies": {
    "@carbon/styles": "^1.39.0",
    "@carbon/web-components": "2.0.1-canary.1",
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "@carbon/grid": "^11.20.0",
    "@carbon/icon-helpers": "^10.44.0",
    "@carbon/icons": "^11.28.0",
    "@carbon/layout": "^11.19.0",
    "@carbon/motion": "^11.15.0",
    "@carbon/themes": "^11.25.0",
    "@carbon/type": "^11.24.0",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@custom-elements-manifest/analyzer": "^0.9.0",
    "@lit/react": "^1.0.1",
    "@open-wc/testing": "^4.0.0",
    "@playwright/test": "^1.39.0",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.5.1",
    "@storybook/web-components": "^7.5.1",
    "@storybook/web-components-vite": "^7.5.1",
    "@types/mocha": "^10.0.3",
    "@types/sinon": "^17.0.0",
    "@web/dev-server-esbuild": "^1.0.0",
    "@web/test-runner": "^0.18.0",
    "@web/test-runner-commands": "^0.9.0",
    "@web/test-runner-playwright": "^0.11.0",
    "axios": "^1.5.1",
    "babel-preset-carbon": "^0.0.14",
    "browserslist-config-carbon": "^11.2.0",
    "commander": "^11.1.0",
    "eslint-config-carbon": "^3.10.0",
    "gitignore-to-glob": "^0.3.0",
    "globby": "^14.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^2.8.8",
    "prettier-config-carbon": "^0.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.29.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-postcss-lit": "^2.1.0",
    "sass": "~1.69.0",
    "sinon": "^17.0.1",
    "storybook": "^7.5.1",
    "stylelint": "^15.11.0",
    "stylelint-config-carbon": "^1.16.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
