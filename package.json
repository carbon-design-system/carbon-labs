{
  "name": "@carbon/ai",
  "version": "0.0.1-alpha.0",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "description": "Carbon for AI",
  "license": "Apache-2.0",
  "repository": "https://github.com/carbon-design-system/carbon-for-ai",
  "main": "es/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es",
    "index.js"
  ],
  "scripts": {
    "build": "vite build --mode development && vite build --mode production",
    "build-storybook": "storybook build",
    "format": "prettier --cache --write '**/*.{js,md,scss,ts,tsx}' '!**/{build,es,lib,.storybook,ts,umd}/**'",
    "format:diff": "prettier --list-different '**/*.{js,md,scss,ts,tsx}' '!**/{build,es,lib,storybook,ts,umd}/**' '!src/components/**'",
    "lint": "eslint src --ext .js,.ts",
    "lint:license": "tasks/check-license.cjs -a",
    "lint:license:staged": "tasks/check-license.cjs -w",
    "lint:styles": "stylelint '**/*.{css,scss}' --report-needless-disables --report-invalid-scope-disables",
    "storybook": "storybook dev -p 6006",
    "prepare": "husky install"
  },
  "dependencies": {
    "@carbon/styles": "^1.39.0",
    "@carbon/web-components": "2.0.1",
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "@carbon/grid": "^11.20.0",
    "@carbon/icon-helpers": "^10.44.0",
    "@carbon/icons": "^11.28.0",
    "@carbon/layout": "^11.19.0",
    "@carbon/motion": "^11.15.0",
    "@carbon/themes": "^11.25.0",
    "@carbon/type": "^11.24.0",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.5.1",
    "@storybook/web-components": "^7.5.1",
    "@storybook/web-components-vite": "^7.5.1",
    "axios": "^1.5.1",
    "babel-preset-carbon": "^0.0.14",
    "browserslist-config-carbon": "^11.2.0",
    "commander": "^11.1.0",
    "eslint-config-carbon": "^3.10.0",
    "fast-glob": "^3.3.1",
    "gitignore-to-glob": "^0.3.0",
    "globby": "^11.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^2.8.8",
    "prettier-config-carbon": "^0.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.29.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-postcss-lit": "^2.1.0",
    "sass": "~1.64.0",
    "storybook": "^7.5.1",
    "stylelint": "^15.11.0",
    "stylelint-config-carbon": "^1.16.0",
    "vite": "^4.4.5"
  }
}
