{
  "name": "@carbon-labs/utilities",
  "version": "0.17.0",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "type": "module",
  "description": "Carbon Labs - utilities",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbon-design-system/carbon-labs",
    "directory": "packages/utilities"
  },
  "main": "./es/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "default": "./es/index.js"
    },
    "./es/*": {
      "types": "./es/*.d.ts",
      "import": "./es/*",
      "default": "./es/*"
    }
  },
  "scripts": {
    "build": "bash scripts/build.sh",
    "clean": "rimraf es"
  },
  "files": [
    "es/**/*.js",
    "es/**/*.d.ts",
    "es/index.js",
    "es/index.d.ts"
  ],
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "babel-cli": "^6.26.0",
    "rimraf": "^6.0.1"
  }
}
