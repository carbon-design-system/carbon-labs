{
  "name": "@carbon-labs/ai-ux-control",
  "version": "0.1.0",
  "private": true,
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "type": "module",
  "description": "Carbon for AI - UX Control component",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
