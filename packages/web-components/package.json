{
  "name": "@carbon-labs/web-components",
  "version": "0.72.0",
  "private": true,
  "type": "module",
  "description": "Web Components for Carbon Labs",
  "license": "Apache-2.0",
  "repository": "https://github.com/carbon-design-system/carbon-labs",
  "types": "es/index",
  "files": [
    "dist",
    "packages/*/es",
    "index"
  ],
  "exports": {
    ".": "./es/index.js",
    "./dist/": "./dist/",
    "./packages/*/es/": "./package/*/es/"
  },
  "scripts": {
    "build": "lerna run build --scope '@carbon-labs/wc-*' --scope '@carbon-labs/ai-chat' --include-dependencies && cem analyze --config ./custom-elements-manifest.config.js",
    "clean": "rimraf es lib storybook-static",
    "storybook": "storybook dev -p 6007",
    "generate": "node tasks/generate",
    "storybook:build": "npx storybook build",
    "test": "yarn build && web-test-runner \"src/components/**/*.test.js\" --node-resolve && yarn test:wc-global-header",
    "test:updateSnapshot": "yarn build && web-test-runner \"src/components/**/*.test.js\" --node-resolve --update-snapshots",
    "test:wc-global-header": "lerna run test --scope '@carbon-labs/wc-global-header'"
  },
  "dependencies": {
    "@carbon/styles": "^1.88.0",
    "lit": "^3.2.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@carbon/grid": "^11.41.0",
    "@carbon/ibm-products-web-components": "^0.23.0",
    "@carbon/icon-helpers": "10.65.0",
    "@carbon/icons": "11.68.0",
    "@carbon/layout": "11.39.0",
    "@carbon/motion": "11.36.0",
    "@carbon/pictograms": "^12.58.0",
    "@carbon/themes": "11.58.0",
    "@carbon/type": "11.45.0",
    "@carbon/web-components": "2.36.0",
    "@custom-elements-manifest/analyzer": "^0.10.0",
    "@lit/react": "^1.0.1",
    "@open-wc/testing": "^4.0.0",
    "@playwright/test": "^1.39.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-replace": "^6.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/pluginutils": "^5.0.5",
    "@storybook/addon-a11y": "^9.1.2",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-links": "^9.1.2",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/web-components-vite": "^9.1.2",
    "@web/test-runner": "^0.20.0",
    "@web/test-runner-commands": "^0.9.0",
    "@web/test-runner-playwright": "^0.11.0",
    "async-done": "^2.0.0",
    "autoprefixer": "^10.4.20",
    "browserslist-config-carbon": "^11.2.0",
    "commander": "^13.0.0",
    "cssnano": "^7.0.0",
    "gitignore-to-glob": "^0.3.0",
    "lerna": "^8.2.3",
    "msw": "^2.10.5",
    "msw-storybook-addon": "^2.0.5",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "replace-ext": "^2.0.0",
    "sass": "~1.89.0",
    "sinon": "^19.0.0",
    "storybook": "^9.1.2",
    "storybook-addon-accessibility-checker": ">=9.2.0-rc.0",
    "through2": "^4.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-lit-css": "^2.1.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
