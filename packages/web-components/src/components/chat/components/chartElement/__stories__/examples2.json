[
  {
    "title": "",
    "prompt": "Make a pair-plot of all columns with scatter plots, use bar distributions when coloumns match",
    "result": {
      "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
      "data": {
        "url": "https://raw.githubusercontent.com/vega/vega-datasets/main/data/cars.json"
      },
      "repeat": {
        "row": [
          "Miles_per_Gallon",
          "Cylinders",
          "Displacement",
          "Horsepower",
          "Weight_in_lbs",
          "Acceleration"
        ],
        "column": [
          "Miles_per_Gallon",
          "Cylinders",
          "Displacement",
          "Horsepower",
          "Weight_in_lbs",
          "Acceleration"
        ]
      },
      "spec": {
        "layer": [
          {
            "mark": {
              "type": "point",
              "tooltip": true
            },
            "encoding": {
              "x": {
                "field": {
                  "repeat": "column"
                },
                "type": "quantitative",
                "scale": {
                  "zero": false
                }
              },
              "y": {
                "field": {
                  "repeat": "row"
                },
                "type": "quantitative",
                "scale": {
                  "zero": false
                }
              }
            }
          },
          {
            "transform": [
              {
                "filter": "datum.repeat_row === datum.repeat_column"
              }
            ],
            "mark": "bar",
            "encoding": {
              "x": {
                "field": {
                  "repeat": "column"
                },
                "bin": true,
                "type": "quantitative"
              },
              "y": {
                "aggregate": "count",
                "type": "quantitative"
              }
            }
          }
        ]
      }
    }
  },
